{
  "version": 2,
  "env": {
  },
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["src/**"] }
    },
    {
      "src": "public/**",
      "use": "@vercel/static",
      "config": { "includeFiles": ["public/**"] }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "./index.js"
    },
    {
      "src": "/",
      "dest": "./index.html"
    },
    {
      "src": "/(.+)",
      "dest": "./$1"
    }
  ]
}
